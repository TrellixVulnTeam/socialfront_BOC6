._a {
  text-decoration: none;
  color: #eb5044;
}
page {
  background-color: #212330;
  color: #ececec;
}
.box {
  padding-top: 220rpx;
  position: relative;
}
.avatar {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.u-button {
  margin-top: 55rpx;
  width: 600rpx;
  border-radius: 60rpx;
  background-color: #eb5044;
  color: #fff;
  font-weight: 500;
  font-size: 35rpx;
  border: 1rpx #eb5044 solid;
}
._span {
  font-size: 15rpx;
  color: #cacaca;
}
.item {
  margin-top: 500rpx;
  margin-left: 40rpx;
}
.pro_box {
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(7, 5, 5, 0.5);
}
.protocol {
  width: 500rpx;
  height: 700rpx;
  background-color: #fff;
  padding: 37rpx;
  border-radius: 40rpx;
}
.protocol ._span:nth-child(1) {
  display: flex;
  justify-content: center;
  font-weight: 600;
  color: black;
  margin-top: 10rpx;
  font-size: 35rpx;
}
.protocol ._span:nth-child(2) {
  display: flex;
  justify-content: center;
  font-weight: 500;
  color: black;
  margin-top: 50rpx;
  font-size: 26rpx;
}
.btn {
  display: flex;
  justify-content: center;
  padding: 0 20rpx;
}
.btn .u-button {
  width: 160rpx;
  height: 60rpx;
  border-radius: 50rpx;
  background-color: #eb5044;
  color: #fff;
  font-weight: 500;
  font-size: 28rpx;
}

